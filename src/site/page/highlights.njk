---
pagination:
  data: collections.highlights
  size: 12
  alias: posts
  addAllPagesToCollections: true
eleventyComputed:
  heading: >-
    {{ theme.highlights.heading }}
  subheading: >-
    {{ theme.highlights.subheading }}
  seo:
    excerpt: >-
      {{ theme.highlights.seo_desc }}
    meta_title: >-
      {{ theme.highlights.seo_title }}
layout: archive.njk
key: archives
permalink: "{{ theme.highlights.slug }}/{% if pagination.pageNumber > 0 %}page-{{ pagination.pageNumber + 1 }}/{% endif %}index.html"
---
