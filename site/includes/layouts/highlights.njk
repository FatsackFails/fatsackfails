---
layout: default
section: highlights
---
<section id="blog-posts" class="flex-1 container ml-auto mr-auto">

  <h1 class="page-title text-3xl md:text-center md:text-5xl mb-16 lg:mb-24 lg:text-5xl">{{ title }}</h1>

  <div class="content">
    {{ content | safe }}
  </div>

  <div class="posts md:flex flex-1">
    {% set postslist = collections.highlights %}
    {% include 'components/postslist.njk' %}
  </div>

</section>
